import timeit
import subprocess

cmd_main = "python main.py < input.txt > output_main.txt"
cmd_optimized = "python main_optimized.py < input.txt > output_optimized.txt"

main_time = timeit.timeit(lambda: subprocess.run(cmd_main, shell=True), number=1)
optimized_time = timeit.timeit(lambda: subprocess.run(cmd_optimized, shell=True), number=1)

print(f"Execution time for main.py: {main_time} seconds")
print(f"Execution time for main_optimized.py: {optimized_time} seconds")